<body>
    <div class='dashboard'>
        <div class="dashboard-nav">
            <header>
                <a href="#!" class="menu-toggle">
                    <i class="fas fa-bars"></i>
                </a>
                <a href="#" class="brand-logo">
                    <i class="fas fa-anchor"></i>
                    <span>PHIEU THU</span>
                </a>
            </header>
            <nav class="dashboard-nav-list">
                <a href="@Url.Action("LoadDSKhoanThu","Home")" class="dashboard-nav-item">
                    <i class="fas fa-home"></i>
                    Home
                </a>
                <a href="@Url.Action("LoadDSKhoanThu","Home")" class="dashboard-nav-item active">
                    <i class="fas fa-tachometer-alt"></i>
                    QL DM Khoản Thu
                </a>
                <div class="nav-item-divider"></div>
                <a href="@Url.Action("DangNhap","Home")" class="dashboard-nav-item"><i class="fas fa-sign-out-alt"></i> Logout </a>
            </nav>
        </div>
        <div class='dashboard-app'>
            <header class='dashboard-toolbar'>
                <a href="#!" class="menu-toggle">
                    <i class="fas fa-bars"></i>
                        <button id="configure" class="k-rpanel-toggle k-button k-primary btn-toggle"><span class="k-icon k-i-menu"></span></button>
                        @Html.ActionLink("QUẢN LÝ DANH MỤC KHOẢN THU", "LoadDSKhoanThu", new { controller = "Home" }, new { @class = "navbar-brand" })

                </a>
            </header>
            <div class='dashboard-content'>
                <div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <footer class="footer text-center d-flex align-items-center">
        <div class="container pb-0">
            <hr />
            <p>Copyright &copy; @DateTime.Now.Year Progress Software Corporation and/or its subsidiaries or affiliates. All Rights Reserved.</p>
        </div>
    </footer>

    <script>
        $(document).ready(function () {
            $("#responsive-panel").kendoResponsivePanel({
                breakpoint: 768,
                autoClose: false,
                orientation: "top"
            });
            $("#menu").kendoMenu();
        });
        function onclick(e) {
            $("#responsive-panel").getKendoResponsivePanel().toggle();
        }
    </script>

</body>

